{
  "version": 2,
  "name": "gangstagram",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/" },
    { "src": "^/(.*)", "dest": "/index.html" }
  ]
}
